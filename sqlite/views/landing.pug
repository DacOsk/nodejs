extends layout.pug

block content
    h1.center SQLite testing
    .center
        form.form(action="/test", method="post")
            label(for="number") Select number
            select(name="number")
                option(value="1") 1
                option(value="2") 2
                option(value="3") 3
                option(value="4") 4
                option(value="5") 5
                option(value="6") 6
                option(value="7") 7
                option(value="8") 8
                option(value="9") 9
                option(value="10") 10
            input.button(type="submit", value="Start test")